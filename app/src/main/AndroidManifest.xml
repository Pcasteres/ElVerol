<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.hotmail.pcasteres.elverol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".app.AppMediator"

        >



        <activity android:name=".PrincipalNoLog.PrincipalNoLogActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListaProductosNoLog.ListaProductosNoLogActivity"
            android:parentActivityName=".PrincipalNoLog.PrincipalNoLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PrincipalNoLog.PrincipalNoLogActivity"/>
        </activity>


    </application>





</manifest>